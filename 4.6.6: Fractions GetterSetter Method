public class FractionTester extends ConsoleProgram
{
    public void run()
    {
        Fraction test = new Fraction(6,13);
        System.out.println(test);
        System.out.println(test.getNumerator());
        System.out.println(test.getDenominator());
        test.setNumerator(7);
        test.setDenominator(8);
        System.out.println(test);
    }
}
